    "better-opn": "^3.0.2",
    "boxen": "^7.1.1",
    "chalk": "^5.4.1",
    "command-exists": "^1.2.9",
    "commander": "^9.5.0",
    "dotenv": "^16.6.1",
    "enquirer": "^2.4.1",
    "esbuild": "^0.25.5",
    "execa": "^8.0.1",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.10",
    "open": "^8.4.2",
    "pako": "^2.1.0",
    "pusher-js": "^7.6.0",
    "tar": "^6.2.1",
    "terminal-kit": "^3.1.2",
    "tiny-invariant": "^1.3.3"
