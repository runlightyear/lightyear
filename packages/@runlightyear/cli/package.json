    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.4",
    "@types/node-fetch": "^2.6.12",
    "@types/nodemon": "^1.19.6",
    "@types/pako": "^2.0.3",
    "@types/tar": "^6.1.13",
    "@types/terminal-kit": "^2.5.7",
    "@typescript-eslint/parser": "^8.35.1",
    "api-extractor-config": "workspace:*",
    "eslint": "^9.30.1",
    "eslint-config-custom": "^0.0.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.2",
    "tsconfig": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
