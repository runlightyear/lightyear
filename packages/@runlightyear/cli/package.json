{
  "name": "@runlightyear/cli",
  "version": "1.8.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "lightyear": "./dist/index.js"
  },
  "scripts": {
    "dev": "nodemon --exec \"npx tsup src/index.ts --dts\" --watch src --ext ts,js",
    "lint": "eslint src/**.js",
    "prettier:check": "prettier --check src",
    "compile": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup src/index.ts --dts"
  },
  "dependencies": {
    "better-opn": "^3.0.2",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "command-exists": "^1.2.9",
    "commander": "^9.5.0",
    "dotenv": "^16.3.1",
    "enquirer": "^2.4.1",
    "esbuild": "^0.25.4",
    "execa": "^8.0.1",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.1",
    "open": "^8.4.2",
    "pako": "^2.1.0",
    "pusher-js": "^8.4.0",
    "tar": "^6.2.1",
    "terminal-kit": "^3.0.1",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@microsoft/api-extractor": "^7.38.5",
    "@types/command-exists": "^1.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.11.24",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemon": "^1.19.2",
    "@types/pako": "^2.0.0",
    "@types/tar": "^6.1.3",
    "@types/terminal-kit": "^2.5.0",
    "@typescript-eslint/parser": "^8.32.1",
    "api-extractor-config": "workspace:*",
    "eslint": "^9.26.0",
    "eslint-config-custom": "*",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  }
}
