{
  "name": "@runlightyear/cli",
  "version": "1.7.3",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "lightyear": "./dist/index.js"
  },
  "scripts": {
