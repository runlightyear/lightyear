{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "tsconfig/base.json",
  "include": ["./"],
  "exclude": ["dist", "build", "node_modules"],
  "compilerOptions": {
    "target": "ES2020",
    "outDir": "./dist",
    "importHelpers": false,
    "resolveJsonModule": true,
    "types": ["vitest"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler"
  }
}
