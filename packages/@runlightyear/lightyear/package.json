{
  "name": "@runlightyear/lightyear",
  "version": "2.0.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nodemon --exec \"npx tsup src/index.ts --dts\" --watch src --ext ts,js",
    "lint": "eslint src/**.ts",
    "prettier:check": "prettier --check src",
    "compile": "tsc --noEmit",
    "build": "tsup src/index.ts --dts",
    "test": "jest",
    "docs:extract": "api-extractor run --local --verbose"
  },
  "dependencies": {
    "babel-jest": "^29.7.0",
    "dayjs": "^1.11.10",
    "graphql": "^16.8.1",
    "json-schema-to-ts": "^3.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "query-string": "^7.1.3",
    "tiny-invariant": "^1.3.1",
    "ts-jest": "^29.1.2",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.38.5",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.182",
    "@types/node": "^20.11.24",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/parser": "^7.1.0",
    "api-extractor-config": "*",
    "eslint": "^8.57.0",
    "eslint-config-custom": "*",
    "jest": "^29.7.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.8.3"
  },
  "description": "The fastest and easiest way for developers to integrate web-based applications.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts"
}
