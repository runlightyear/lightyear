{
  "name": "@runlightyear/lightyear",
  "version": "0.38.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nodemon --exec \"npx tsup src/index.ts --dts\" --watch src --ext ts,js",
    "lint": "eslint src/**.ts",
    "prettier:check": "prettier --check src",
    "compile": "tsc --noEmit",
    "build": "tsup src/index.ts --dts",
    "test": "jest",
    "docs:extract": "api-extractor run --local --verbose"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "babel-jest": "^28.1.3",
    "dayjs": "^1.11.10",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "query-string": "^7.1.3",
    "tiny-invariant": "^1.3.1",
    "ts-jest": "^28.0.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.38.1",
    "@types/aws-lambda": "^8.10.125",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.200",
    "@types/node": "12.20.24",
    "@types/node-fetch": "^2.6.8",
    "@typescript-eslint/parser": "^5.62.0",
    "api-extractor-config": "^0.0.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "^0.0.0",
    "jest": "^28.1.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  },
  "description": "The fastest and easiest way for developers to integrate web-based applications.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts"
}
