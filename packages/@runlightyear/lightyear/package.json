{
  "name": "@runlightyear/lightyear",
  "version": "0.33.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nodemon --exec \"npx tsup src/index.ts --dts\" --watch src --ext ts,js",
    "lint": "eslint src/**.ts",
    "prettier:check": "prettier --check src",
    "compile": "tsc --noEmit",
    "build": "tsup src/index.ts --dts",
    "test": "jest",
    "docs:extract": "api-extractor run --local --verbose"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "babel-jest": "^28.1.3",
    "dayjs": "^1.11.4",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "query-string": "^7.1.1",
    "tiny-invariant": "^1.2.0",
    "ts-jest": "^28.0.7",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.31.2",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "12.20.24",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/parser": "^5.38.0",
    "api-extractor-config": "*",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsup": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "description": "The fastest and easiest way for developers to integrate web-based applications.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts"
}
