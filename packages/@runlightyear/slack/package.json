{
  "name": "@runlightyear/slack",
  "version": "0.16.2",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nodemon --exec \"npx tsup src/index.ts --dts\" --watch src --ext ts,js",
    "lint": "eslint src/**.ts",
    "prettier:check": "prettier --check src",
    "compile": "tsc --noEmit",
    "build": "tsup src/index.ts --dts",
    "docs:extract": "api-extractor run --local --verbose"
  },
  "dependencies": {
    "@runlightyear/lightyear": "workspace:*",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.38.2",
    "@types/jest": "^28.1.8",
    "@types/node": "12.20.24",
    "@types/node-fetch": "^2.6.8",
    "@types/nodemon": "^1.19.4",
    "@typescript-eslint/parser": "^5.62.0",
    "api-extractor-config": "workspace:*",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "nodemon": "^3.0.1",
    "tsconfig": "workspace:*",
    "tsdoc-config": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts"
}
